@model dethi1920.Models.CongNhanModel


@{
    ViewData["Title"] = "View";

}
<form acton="/DiemCachLy/LietKe" method="post">
    <div class="form-group">
        <label>Mã Công Nhân</label>
        <input type="hidden" class="from-control" value="@Model.MaCongNhan id="MaCongNhan" name="MaCongNhan" />
    </div>
    <div class="form-group">
        <label>Tên Công nhân</label>
        <input type="text" class="from-control" value=" @Model.TenCongNhan" id="tenPhim" name="tenPhim" />
    </div>
    <div class="form-group">
        <label>Giới Tính</label>
        <input type="text" class="from-control" value="@Model.GioiTinh" id="thoiLuong" name="thoiLuong" />
    </div>
    <div class="form-group">
        <label>Năm Sinh</label>
        <input type="date" class="from-control" value="@Model.NamSinh" id="ngonNgu" name="ngonNgu" />
    </div>
    <div class="form-group">
        <label>Nước về</label>
        <input type="text" class="from-control" value="@Model.NuocVe" id="theLoai" name="theLoai" />
    </div>



</form>
<div>
    <a asp-action="LietKe">Back to List</a>
</div>